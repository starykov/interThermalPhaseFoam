FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    Inlet
    {
		type		cyclic;
    }

    BotOutlet
    {
		type		cyclic;
    }
 
    FreeStream
    {
        type        zeroGradient;
    }

    FrontNBack
    {
        type            empty;
    }

    LeftSide
    {
        type            fixedValue;
        value           uniform 293;
    }
}


