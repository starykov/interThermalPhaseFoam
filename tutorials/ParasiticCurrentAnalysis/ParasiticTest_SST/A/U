FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	Walls
	{
		type            fixedValue;
		value           uniform (0 0 0);
	}

	SymPlane1
	{
		type            symmetryPlane;
		value           uniform (0 0 0);
	}

	SymPlane2
	{
		type            symmetryPlane;
		value           uniform (0 0 0);
	}

	topNbot
	{
		type            empty;
	}
}


