FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Walls
    {
        type            fixedGradient;
        gradient           uniform 0;
		//type           fixedValue;
		//value          uniform 0;
    }

	SymPlane1
	{
		type            symmetryPlane;
		value           uniform 0;
	}

	SymPlane2
	{
		type            symmetryPlane;
		value           uniform 0;
	}


    defaultFaces
    {
        type            empty;
    }
}

